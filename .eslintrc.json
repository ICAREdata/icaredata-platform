{
  "env": {
    "browser": false,
    "commonjs": true,
    "es6": true
  },
  "extends": ["google", "plugin:prettier/recommended"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "max-len": ["error", { "code": 200 }],
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["__get__", "__set__"]
      }
    ]
  }
}
